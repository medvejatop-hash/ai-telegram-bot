const express = require('express');
const app = express();
const PORT = process.env.PORT || 10000;

app.use(express.json());

app.post('/webhook', (req, res) => {
  console.log('Webhook received');
  res.send('OK');
});

app.get('/', (req, res) => {
  res.send('Server working!');
});

app.listen(PORT, () => {
  console.log('Server started on port ' + PORT);
});
