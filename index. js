const express = require('express');
const app = express();
app.use(express.json());

app.post('/webhook', (req, res) => {
  console.log('✅ WEBHOOK WORKS!');
  res.send('OK');
});

app.get('/', (req, res) => {
  res.send('SERVER WORKS!');
});

const PORT = process.env.PORT || 10000;
app.listen(PORT, () => {
  console.log('✅ SERVER STARTED');
});
